{
  "name": "reactGetStarted",
  "description": "Rest API",
  "version": "0.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "docs": "./node_modules/apidoc/bin/apidoc -i routes -o doc/",
    "start": "node server.js",
    "test": "set NODE_ENV=test& node ./node_modules/mocha/bin/_mocha -- 'test/**/*.js'",
    "test-win": "set NODE_ENV=test& node ./node_modules/mocha/bin/_mocha -- test/**/*.js",
    "coverage": "set NODE_ENV=test& node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- test/**/*.js",
    "coverage-win": "set NODE_ENV=test& node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- test/**/*.js"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "react-tools": "^0.13.3",
    "serve-favicon": "~2.3.0",
    "winston": "latest",
    "mongoose": "latest",
    "async": "latest"
  },
  "apidoc": {
    "title": "Rest api",
    "url": "https://",
    "sampleUrl": "https://"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "4.3.0",
    "istanbul": "0.4.3",
    "mocha": "2.4.5",
    "mongodb": "2.1.11",
    "nock": "8.0.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "supertest": "1.2.0"
  }
}
